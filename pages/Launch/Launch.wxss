/* pages/Launch/Launch.wxss */

Page {
  background-color: #fbc647;
  background-image: url("https://mq.mouqukeji.com/static/image/launch.png");
  background-position: 50%;
  background-size: cover;
}

button {
  width: 35%;
  height: 100rpx;
  padding: 0;
  position: fixed;
  bottom: 440rpx;
  left: 0;
  right: 0;
  color: #161525;
  font-size: 28rpx;
  line-height: 96rpx;
  background-color: #fbc647;
  background-image: url("https://mq.mouqukeji.com/static/image/login.png");
  background-repeat: no-repeat;
  background-position: 50% 50%;
  background-size: cover;
}

text.login {
  width: 100%;
  height: 50%;
  display: inline-block;
  margin: 0 auto;
  font-size: 28rpx;
  text-align: center;
}

text.timerest {
  color: #161525;
  font-size: 28rpx;
}

button::after {
  border: none;
}

.button-hover {
  color: #161525;
  background-color: #fbc647;
  background-image: url("https://mq.mouqukeji.com/static/image/login.png");
  background-repeat: no-repeat;
  background-position: 50% 48%;
  background-size: cover;
}

.button-hover text.login {
  margin-top: 2%;
}

button[disabled]:not([type]) {
  color: #161525;
  background-color: #fbc647;
}
