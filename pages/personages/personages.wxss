text{
  font-size: 14px;
  font-family:"Microsoft Yahei";
}
page {
  width: 100%;
  height: 100%;
  overflow-x: hidden;
  background-color: #ededed;
}
.skip{
  width: 94%;
  height:40%;
  margin: 20px auto 0rpx;
  /* background:#fff; */
}
.identity{
  display: flex;
  flex-direction: row;
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  padding: 40rpx 0rpx 40rpx;
  background:#fff;
  /* margin-top:5px; */
  /* border-bottom: 1px solid #ccc; */
}
.identity:active{
  background-color: #ddd;
}
.identity .arrow{
  width: 16rpx;
  height: 16rpx;
  border-top: 2px solid #999;
  border-right: 2px solid #999;
  position: absolute;
  right: 40rpx;
  transform: rotate(45deg);
  margin-top: 4px;
}
.identity image{
  margin-top: 19rpx;
  margin-left:12%;
}