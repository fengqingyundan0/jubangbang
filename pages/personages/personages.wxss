/* pages/personages/personages.wxss */
.mask{
  width:100%;
  height:100%;
  position:fixed;
  background-color:#f5f1f2;
  z-index:9999;
  top:0;
  left:0;
  opacity: 1;
}
.contain{
  display: flex;
  flex-direction: column;
  align-items: center;
  font-size: 19px;
  padding: 20px 20px;
  font-weight: bold;
  background:#fff;
}
.contain image{
  width: 150rpx;
  height: 150rpx;
  border-radius: 75rpx;
}
.message{
  display: flex;
  flex-direction: row;
  margin-top: 10px;
}
.status{
  width: 68px;
  height: 20px;
  line-height: 20px;
  color:#fff;
   text-align: center;
   border-radius: 5px;
  background-color:#5ea5fc;
}
.ntegration{
  width: 40px;
  height: 20px;
  line-height: 20px;
  color: #fff;
  border-radius: 5px;
  margin-left: 20px;
  background-color:#5ea5fc;
  text-align: center;
}
