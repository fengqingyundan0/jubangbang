page{
  width: 100%;
  height: 100%;
  background-color: #ededed;
}
.mask{
  width:100%;
  height:100%;
  position:fixed;
  background-color:#f5f1f2;
  z-index:9999;
  top:0;
  left:0;
  opacity: 1;
}
.part{
  width: 100%;
  height: 140rpx;
  margin-top: 10rpx;
  background: #fff;
}
.parts{
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 36rpx;
  display: flex;
  flex-direction: row;
  padding-top: 20rpx;
}
.part_t1{
  width: 320rpx;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 36rpx;
  display: inline-block;
  margin-left: 6%;
}
.part_t3{
  width: 220rpx;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 36rpx;
  display: block;
  margin-left: 16%;
  text-align: right;
}
.partz{
  width: 100%;
  height: 40rpx;
  display: flex;
  flex-direction: row;
}
.part_t2{
  width: 320rpx;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 28rpx;
  display: block;
  color: #ccc;
  margin-left: 6%;
}
.part_t4{
  width: 220rpx;
  margin-left: 16%;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 28rpx;
  display: block;
  color: #ccc;
  text-align: right;
}