/* pages/Index/Index.wxss */

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.mask {
  width: 100%;
  height: 100%;
  position: fixed;
  background-color: #f5f1f2;
  z-index: 9999;
  top: 0;
  left: 0;
  opacity: 1;
}

page {
  width: 100%;
  height: 1800rpx;
  overflow-x: hidden;
  background-color: #eee;
}

.placeholderZindex {
  z-index: 1;
}

.nav {
  width: 100%;
  display: flex;
  flex-direction: row;
  height: 88rpx;
  background-color: white;
  border-bottom: 1px solid #e8e8e8;
}

.tab_name {
  display: flex;
  flex-direction: row;
  margin: 2px 20rpx;
}

.tab_name image {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
}

.nav swiper-item {
  width: 90%;
  top: 20rpx;
  text-align: center;
  font-size: 16px;
  color: #999;
}

.navbar {
  width: 100%;
  background: #fff;
  white-space: nowrap;
  position: relative;
}

.navbar_slider {
  position: absolute;
  width: 160rpx;
  height: 2px;
  content: "";
  left: 0;
  transition: transform 0.3s;
}

.navbar .item {
  height: 70rpx;
  line-height: 70rpx;
  color: #ccc;
  font-size: 26rpx;
  margin: 0 8rpx;
  display: inline-block;
  text-align: center;
}

.navbar .item.active {
  color: #e34545;
  /* color:#000; */
  border-bottom: 6rpx solid #ff5656;
  /* border-bottom: 6rpx solid #000; */
}

.nav .currentCity {
  width: 20%;
  line-height: 88rpx;
  font-size: 14px;
  color: #ff5656;
  position: absolute;
  right: 0px;
  text-align: center;
}

.car {
  width: 100%;
  height: 318rpx;
  overflow: hidden;
  z-index: 1;
  /* position: relative; */
}

.car .car_type {
  width: 100%;
  height: 318rpx;
  background-color: #fff;
  /* position: absolute; */
}

.car .weight, .size, .volum {
  font-size: 28rpx;
  width: 33%;
  text-align: center;
}

.car .weight text, .size text, .volum text {
  display: block;
}

.car .tp {
  font-size: 24rpx;
  color: #ccc;
}

.car .suffer {
  width: 25%;
  position: fixed;
  top: 170rpx;
  right: 30rpx;
  font-size: 20rpx;
  color: #ccc;
}

.car-indicator-dots {
  width: 100%;
  position: relative;
}

